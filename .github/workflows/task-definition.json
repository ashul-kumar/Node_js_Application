{
  "containerDefinitions": [
    {
      "name": "node-app",
      "image": "<Jfrog_image:latest>",
      "repositoryCredentials": {
      "credentialsParameter": "<ARN_OF_YOUR_SECRET"
    },
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
         }
        ],
        "essential": true,
        "environment": [],
        "environmentFiles": [],
        "mountPoints": [],
        "volumesFrom": [],
        "ulimits": []
       }
    ],
    "family": "node-app-deploy-studentXX",
    "executionRoleArn":
    "arn:aws:iam::340704870584:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "tags": [
       {
      "key": "Name",

      "value": "my-node-app"
     }
    ]
  }
